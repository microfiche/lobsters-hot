<li id="story_xiaimw" data-shortid="xiaimw"
class="story 






">
<div class="story_liner h-entry">
  <div class="voters">
      <a class="upvoter" href="/login"></a>
    <div class="score">49</div>
  </div>
  <div class="details">
    <span class="link h-cite u-repost-of">
