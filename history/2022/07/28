<li id="story_hbtyza" data-shortid="hbtyza"
class="story 






">
<div class="story_liner h-entry">
  <div class="voters">
      <a class="upvoter" href="/login"></a>
    <div class="score">3</div>
  </div>
  <div class="details">
    <span class="link h-cite u-repost-of">
        <a class="u-url" href="https://courses.cs.washington.edu/courses/cse590p/05au/p50-bacon.pdf" rel="ugc noreferrer">A Unified Theory of Garbage CollectIon (2004)</a>
    </span>
      <span class="tags">
          <a class="tag tag_pdf tag_is_media" title="Link to a PDF document" href="/t/pdf">pdf</a>
          <a class="tag tag_plt" title="Programming language theory, types, design" href="/t/plt">plt</a>
      </span>
        <a class="domain" href="/domain/courses.cs.washington.edu">courses.cs.washington.edu</a>



    <div class="byline">
        <a href="/u/lorddimwit"><img srcset="/avatars/lorddimwit-16.png 1x, /avatars/lorddimwit-32.png 2x" class="avatar" alt="lorddimwit avatar" loading="lazy" decoding="async" src="/avatars/lorddimwit-16.png" width="16" height="16" /></a>
