<li id="story_mdxlct" data-shortid="mdxlct"
class="story 






">
<div class="story_liner h-entry">
  <div class="voters">
      <a class="upvoter" href="/login"></a>
    <div class="score">24</div>
  </div>
  <div class="details">
    <span class="link h-cite u-repost-of">
        <a class="u-url" href="https://github.com/dantecatalfamo/zig-dns" rel="ugc noreferrer">zig-dns: Experimental DNS library implemented in zig</a>
    </span>
      <span class="tags">
          <a class="tag tag_zig" title="Zig programming" href="/t/zig">zig</a>
      </span>
        <a class="domain" href="/domains/github.com">github.com</a>



    <div class="byline">
        <a href="/u/dante"><img srcset="/avatars/dante-16.png 1x, /avatars/dante-32.png 2x" class="avatar" alt="dante avatar" loading="lazy" decoding="async" src="/avatars/dante-16.png" width="16" height="16" /></a>
