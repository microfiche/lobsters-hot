<li id="story_uf3xxu" data-shortid="uf3xxu"
class="story 






">
<div class="story_liner h-entry">
  <div class="voters">
      <a class="upvoter" href="/login"></a>
    <div class="score">1</div>
  </div>
  <div class="details">
    <span class="link h-cite u-repost-of">
        <a class="u-url" href="https://boston.conman.org/2022/12/21.1" rel="ugc noreferrer">Unit test this</a>
    </span>
      <span class="tags">
          <a class="tag tag_c" title="C programming" href="/t/c">c</a>
          <a class="tag tag_linux" title="Linux" href="/t/linux">linux</a>
          <a class="tag tag_practices" title="Development and team practices" href="/t/practices">practices</a>
          <a class="tag tag_testing" title="Software testing" href="/t/testing">testing</a>
      </span>
        <a class="domain" href="/domains/boston.conman.org">boston.conman.org</a>



    <div class="byline">
        <a href="/u/spc476"><img srcset="/avatars/spc476-16.png 1x, /avatars/spc476-32.png 2x" class="avatar" alt="spc476 avatar" loading="lazy" decoding="async" src="/avatars/spc476-16.png" width="16" height="16" /></a>
