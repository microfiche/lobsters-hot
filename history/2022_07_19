<li id="story_1ot8rq" data-shortid="1ot8rq"
class="story 






">
<div class="story_liner h-entry">
  <div class="voters">
      <a class="upvoter" href="/login"></a>
    <div class="score">16</div>
  </div>
  <div class="details">
    <span class="link h-cite u-repost-of">
        <a class="u-url" href="https://catgirl.ai/log/comfy-software/" rel="ugc noreferrer">Comfy Software</a>
    </span>
      <span class="tags">
          <a class="tag tag_culture" title="Technical communities and culture" href="/t/culture">culture</a>
      </span>
        <a class="domain" href="/domain/catgirl.ai">catgirl.ai</a>



    <div class="byline">
        <a href="/u/quasi_qua_quasi"><img srcset="/avatars/quasi_qua_quasi-16.png 1x, /avatars/quasi_qua_quasi-32.png 2x" class="avatar" alt="quasi_qua_quasi avatar" loading="lazy" decoding="async" src="/avatars/quasi_qua_quasi-16.png" width="16" height="16" /></a>
