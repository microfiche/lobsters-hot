  <div class="voters">
      <a class="upvoter" href="/login"></a>
    <div class="score">9</div>
  </div>
  <div class="details">
    <span role="heading" aria-level="1" class="link h-cite u-repost-of">
        <a class="u-url" href="https://two-wrongs.com/types-as-interfaces" rel="ugc noreferrer">Types as Interfaces</a>
