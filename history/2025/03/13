  <div class="voters">
      <a class="upvoter" href="/login"></a>
    <div class="score">6</div>
  </div>
  <div class="details">
    <span role="heading" aria-level="1" class="link h-cite u-repost-of">
        <a class="u-url" href="https://afmoreno.srht.site/blog/guile-hoot-fibers.html" rel="ugc noreferrer">Guile Hoot with Fibers</a>
